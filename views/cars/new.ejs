<%- include ../partials/header %>
<div class= "container">
	<div class="row">Make a new account</div>
	<div class="col-md-6 col-md-offset-3">
        <form role="form">
            <div class="form-group">
                <label for="model">Make and Model</label>
                <input type="text" class="form-control" id="model" placeholder="Toyota Corolla">
            </div>
            <div class="form-group">
                <label for="year">Year</label>
                <input type="number" class="form-control" id="year" placeholder="2011">
            </div>
            <div class="form-group">
                <label for="year">Color</label>
                <input type="text" class="form-control" id="color" placeholder="Red">
            </div>
            <div class="form-group">
                <label for="year">How much are you selling for?</label>
                <input type="number" class="form-control" id="Price" placeholder="9500">
            </div>
            <div class="form-group">
                <label for="VIN">Vehicle VIN Number</label>
                <input type="text" class="form-control" id="VIN" placeholder="ABCD123405">
            </div>            
            <div class="form-group">
                <label for="exampleInputFile">Upload an image</label>
                <input type="file" id="exampleInputFile">
                <p class="help-block">Example block-level help text here.</p>
            </div>
            <div class="checkbox">
                <label>
              <input type="checkbox"> I agree to the terms and conditions
            </label>
            </div>
            <button type="submit" class="btn btn-primary">Add this car</button>
        </form>
	</div>
</div>
<%- include ../partials/footer %>