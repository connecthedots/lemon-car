<%- include partials/header %>
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                <div class="thumbnail">
                    <img src="<%=lemon.image%>">
                </div>
                <ul>
                    <li>Make and Model: <%=lemon.model%></li>
                    <li>Year: <%=lemon.year%></li>
                    <li>Color: <%=lemon.color%></li>
                    <li>VIN: <%=lemon.VIN%></li>
                    <li>Price: $<%=lemon.price%></li>
                </ul>
                <div>
                    <a class="btn btn-lg btn-primary" href="/register/<%=lemon._id%>">Get this car</a>
                </div>
            </div>
        </div>
    </div>
<%- include partials/footer %>